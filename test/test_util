#!/usr/bin/env bash

if [[ -n ${TEST_VERBOSE+x} ]]; then
    set -o verbose
fi

EMGR_DIRS=( "$TEST_DIR/envs" )

function tc_fail() {
    echo "Failed!"
    exit 1
}

function emgr_setup() {
    source "$TEST_DIR/../emgr_setup"
}

function check_var_set() {
    if typeset -p "$1" >&/dev/null; then
        return 0
    else
        return 1
    fi
}
